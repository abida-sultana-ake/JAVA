// Properties
public class car
{
    int noOfWheels;
    String color;
    float maxSpeed;
    float currentFuelInLiters;
    int noOfSeats;
    
    // method
    public void drive()
    {
        System.out.println("Car is driving");
        currentFuelInLiters --;
    }
    public void addFuel(float fuel)
    {
        currentFuelInLiters += fuel;
    }
    public float getCurrentFuleLevel()
    {
        return currentFuelInLiters;
    }
}
